<%- include('./partials/head2.ejs') %>

<style>
    .orders{
        display: grid;
        grid-template-columns: 90rem auto;
        gap: 1.6rem;
    }

    .infos{
        background-color: var(--color-white);
        border-radius: var(--card-border-radius);
        padding: var(--padding-1);
        box-shadow: var(--box-shadow);
        transition: all 300ms ease;
    }

    .infos>div{
        display: flex;
        justify-content: space-between;
        gap: .5rem;
        border-bottom: 1px solid var(--color-light);
        padding: 1rem;
    }

    .infos>div:last-child{
        border-bottom: none;
    }

    .infos:hover{
        box-shadow: none;
    }

    @media (max-width: 1310px){
        .orders{
            grid-template-columns: 53rem auto;
            gap: 1.6rem;
        }
    }
</style>

<body>

    <div class="container2">
        <%- include('./partials/navteste.ejs') %>

        <main>
            <%- include('./partials/rightmenu.ejs') %>
            <h1>Dashboard</h1>
            <br>
            <div style="display: flex; flex-direction: column; justify-content: center; align-items: center; height: 80vh;">
                <span style="font-size: 90px;" class="material-icons-sharp">warning</span>
                <h1>Dashboard em manutenção</h1>
            </div>
        </main>
    </div>
    <script src="/src/js/script.js"></script>
</body>
</html>